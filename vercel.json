{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "framework": "remix",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "app/routes/api.chat.ts": {
      "maxDuration": 60
    },
    "app/routes/api.enhancer.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "OPENROUTER_API_KEY": "@openrouter_api_key"
  }
} 